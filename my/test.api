api Test {
  assert_not: Bool -> Bool;
  assert_strings: ( Bool, String, String ) -> Bool;
  assert_strings_are_equal: ( String, String ) -> Bool;
  assert_strings_are_inequal: ( String, String ) -> Bool;
  test_assert_strings: Void -> Bool;
  assert_lists_of_strings: ( Bool, List( String ), List( String ) ) -> Bool;
  assert_lists_of_strings_are_equal: ( List( String ), List( String ) ) -> Bool;
  assert_lists_of_strings_are_inequal: ( List( String ), List( String ) ) -> Bool;
  assert_ints: ( Bool, Int, Int ) -> Bool;
  assert_ints_are_equal: ( Int, Int ) -> Bool;
  assert_ints_are_inequal: ( Int, Int ) -> Bool;
  test_assert_ints: Void -> Bool;
  assert_bools: ( Bool, Bool, Bool ) -> Bool;
  assert_bools_are_equal: ( Bool, Bool ) -> Bool;
  assert_bools_are_inequal: ( Bool, Bool ) -> Bool;
  test_assert_bools: Void -> Bool;
  test_assert_lists_of_strings: Void -> Bool;
  is_even: Int -> Bool;
  test_is_even: Void -> Bool;
  is_odd: Int -> Bool;
  test_is_odd: Void -> Bool;
  test_all_tests: Void -> Bool;
};
